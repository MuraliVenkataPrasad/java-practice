import java.util.Scanner;
public class mail {
	public static void main(String[] args) {
	System.out.print("enter a mail :");
	Scanner sc=new Scanner(System.in) ;
	String a =sc.next();
	for(int i = 0; i < a.length();i++) {
		if(a.charAt(i)=='@') {
			for(int j=i+1;j<a.length();j++) {
				System.out.print(a.charAt(j));
			}
		}
	}
	}
}
